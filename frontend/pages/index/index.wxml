<!--index.wxml-->
<view class="{{skinStyle}}">
  <view class="contain">
    <view  bindtap="bindViewTap" class="userinfo">
      <image class="userinfo-avatar" src="{{userInfo.avatarUrl}}" background-size="cover"></image>
      <text class="userinfo-nickname">{{userInfo.nickName}}</text>
    </view>
    <view>
      <image class="imagesize image" src="../../figures/cube.png"></image>
    </view>
    <view class="title">{{WelcomeText}}</view>
    <view class="subtitle">{{ProductNameText}}</view>
    <button class='btn' bindtap='ButtonClick'>{{LoginText}}</button>
  </view>
</view>